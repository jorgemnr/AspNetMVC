@model System.Web.Mvc.HandleErrorInfo
@{
	Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Error</title>
</head>
<body>
    <hgroup>
        <h1 class="text-danger">Erro.</h1>
        <h2 class="text-danger">Um erro ocorreu enquanto processava a solicitação.</h2>
    </hgroup>
	<p>@Model.Exception.GetBaseException().ToString()</p>
</body>
</html>
